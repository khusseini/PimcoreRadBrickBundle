{
    "name": "khusseini/pimcore-radbrick",
    "description": "A RAD way to create areabricks in Pimcore",
    "type": "pimcore-bundle",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Khair-ed-Din Husseini",
            "email": "kmhusseini@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "Khusseini\\PimcoreRadBrickBundle\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Tests\\Khusseini\\PimcoreRadBrickBundle\\": ["src/"]
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^0.12.17",
        "pimcore/pimcore": "^6",
        "sebastian/phpcpd": "^5.0",
        "infection/infection": "^0.16.2",
        "php-coveralls/php-coveralls": "^2.2"
    }
}
